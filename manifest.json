{
    "manifest_version": 2,
    "name": "Fund Helper",
    "version": "0.1",
    "description": "A helper for tiantian fund",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png"
        },
        "default_title": "Fund Helper",
        "default_popup": "popup.html"
    },
    "permissions": [
        "cookies",
        "https://*.eastmoney.com/*",
        "https://*.1234567.com.cn/*"
    ],
    // "chrome_url_overrides" : {
    //     "newtab": "popup.html"
    // },
    "options_page": "options.html",
    "options_ui": {
        // "chrome_style": true,
        "page": "options.html"
    }
}
